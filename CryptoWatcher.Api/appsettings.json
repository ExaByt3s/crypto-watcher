{
  // App settings
  "Authorize": "True",
  "JobsIntervalInMinutes": "1",
  "TelegramApiToken": "my_bot_api_token",

  // Connection strings
  "ConnectionStrings": {
    "CryptoWatcher": "Data Source=your_server;Initial Catalog=your_database;Integrated Security=True"
  },

  // Logging
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "None",
      "Microsoft": "None",
      "Hangfire": "None"
    }
  }
} 
